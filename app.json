{
  "expo": {
    "name": "Dr. Persson",
    "slug": "DrPersson",
    "githubUrl": "https://github.com/raisanen/drpersson",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffff00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.fivebyfive.drpersson",
      "supportsTablet": true
    },
    "android": {
      "package": "com.fivebyfive.drpersson",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffff00"
      },
      "permissions": []
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffff00"
    }
  }
}